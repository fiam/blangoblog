#: blango/email.py:17
#, python-format
msgid "New comment on %s"
msgstr "Nuevo comentario en %s"

#: blango/email.py:20
#, python-format
msgid ""
"\n"
"        A new comment has been posted in %(entry)s. You can read it at %(uri)"
"s\n"
"\n"
"        \n"
"        You're receiving this email because you subscribed to \"%(entry)s\"\n"
"        in %(blog_title)s. If you no longer want to received any followups\n"
"        just visit %(unsubscribe)s\n"
"        "
msgstr ""
"\n"
"Se ha añadido un nuevo comentario en %(entry)s. Puedes leerlo en %(uri)s\n"
"\n"
"       Estás recibiendo este correo porque te suscribiste a \"%(entry)s\"\n"
"       en %(blog_title)s. Si no deseas seguir recibiendo estos avisos,\n"
"       visita %(unsubscribe)s\n"
"       "

